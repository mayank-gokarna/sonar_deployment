<%= render :partial => 'gwt/base', :locals => {:resource => @resource, :popup => true, :metric => (@metric && @metric.key)} -%>
<%= render :partial => 'gwt/resource_viewers' -%>
<div id="accordion-panel"/>
<script>
$j(document).ready(function() {
// Always display title in popup mode
openAccordionItem('<%= url_for(params.merge({:display_title => 'true', :popup => 'true'})) -%>', this);
});
// refresh page after issues bulk change
function onBulkIssues(){
resourceViewerOnBulkIssues();
}
</script>
